{
  "compilerOptions": {
    "target": "ES2017",  
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "outDir": "dist"
  }
}
